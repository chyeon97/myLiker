<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item    android:state_focused="false" android:state_enabled="false">
        <shape>
            <solid android:color="@color/colorDarkGrayLiker"/>
            <corners android:radius="10dp" />
        </shape>
    </item>

    <!--평상시-->
    <item android:state_focused="false" android:state_enabled="true" android:state_pressed="false">
        <shape>
            <solid android:color="@color/colorYellowLiker" />
            <corners android:radius = "10dp"/>
        </shape>
    </item>

    <item android:state_focused="false" android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorYellowLiker_push"/>
            <corners android:radius="10dp"/>
        </shape>

    </item>


</selector>